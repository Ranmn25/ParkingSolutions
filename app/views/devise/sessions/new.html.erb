<h2>Log in</h2>

<div class="container">
  <div class="col-sm-4 col-sm-offset-4">
    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
     <%= image_tag("logo1.png", :alt => "logo") %>
        <%= f.input :email,
                    required: false,
                    autofocus: true,
                    input_html: { autocomplete: "email", class: "login-from-input" },
                    wrapper_html: { class: "form-group" } %>
        <%= f.input :password,
                    required: false,
                    input_html: { autocomplete: "current-password" } %>
                     <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
                      <%= f.button :submit, "Log in", class: "login-btn" %>

<%#
      <div class="form-actions"> %>


    <% end %>
    <div class="form-group form-footer">
    <div class="form-group form-footer">
        <p>Dont have an account? <a href="" title="">Create Account</a></p>
        <p><a href="" title="">Forgot Password?</a></p>
            <%# <%= link_to '', booking_path(booking, {status: 'accepted'}), method: :patch %> %>

        <%# <%= link_to %> %>
      </div>
    <%# <%= render "devise/shared/links"%> %>
    </div>
  </div>
</div>



